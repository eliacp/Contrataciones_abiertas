    <label class="checkbox-inline"><input name="implementationUpdate" type="checkbox" <%= tags.implementationupdate?'checked':'' %> >implementationUpdate</label><br>
    <label class="checkbox-inline"><input name="contractTermination" type="checkbox" <%= tags.contracttermination?'checked':'' %> >contractTermination</label><br>
    <label class="checkbox-inline"><input name="compiled" type="checkbox" <%= tags.compiled?'checked':'' %> >compiled</label>
</div>
    <p>
        <button type="submit" class="btn btn-default">Guardar</button>
    </p>
</form>